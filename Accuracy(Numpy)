def accuracy(y_true, y_pred):
    y_true_array=np.array(y_true)
    y_pred_array=np.array(y_pred)
    return np.mean(y_true_array==y_pred_array)
    # ................................................................
    """A function which takes two lists and returns the accuracy"""

